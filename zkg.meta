[package]
description = a PDF file analyzer for Zeek
tags = bro plugin, file analyzer, pdf
script_dir = scripts/Analyzer/PDF
build_command = ./configure --zeek-dist=%(zeek_dist) --with-podofo=%(PODOFO_PREFIX) && make
user_vars =
  PODOFO_PREFIX [/usr/include] "PoDoFo install prefix"
external_depends =
  podofo >=0.9.6
